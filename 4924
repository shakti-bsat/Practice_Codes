a=input()
b=input()
c=input()
for i in a:
    for j in c:
        if i==j:
          a=a.replace(i,"")
          c=c.replace(j,"")
for i in b:
    for j in c:
        if i==j:
            b=b.replace(i,"")
            c=c.replace(j,"")

for i in a:
    for j in b:
        if i==j:
          a=a.replace(i,"")
          b=b.replace(j,"")
p=a+b
print(p)
